<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dino.rxsample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".week5.debounce.DebounceActivity"
            android:label="DebounceActivity" />
        <activity
            android:name=".week5.throttle.ThrottleActivity"
            android:label="ThrottleActivity" />
        <activity
            android:name=".week5.buffer.BufferActivity"
            android:label="BufferActivity" />
        <activity
            android:name=".week5.combinelatest.CombineLatestActivity"
            android:label="CombineLatestActivity" />
        <activity
            android:name=".week5.behaviorsubject.BehaviorSubjectActivity"
            android:label="BehaviorSubjectActivity" />
        <activity
            android:name=".week6.retrofit.RetrofitActivity"
            android:label="RetrofitActivity" />
        <activity
            android:name=".week6.rxmvvm.RxMvvmActivity"
            android:label="RxMvvmActivity" />
        <activity android:name=".week5.behaviorsubject.SenderActivity" />
    </application>

</manifest>